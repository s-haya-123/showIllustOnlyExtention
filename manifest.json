{
    "name": "Chrome Ext",
    "manifest_version": 2,
    "description": "タブの情報を表示します",
    "version": "1.0",
    "content_scripts": [
        {
            "matches":  [ "https://twitter.com/*"],
            "all_frames": true,
            "js": [
                "bundle.js"
            ],
            "css":[
            ]
        }
    ],
    "background": {
      "scripts": []
    },
    "web_accessible_resources": [
      "model/*",
      "sample.json"
    ],
    "permissions": [
      "tabs",
      "contextMenus"
    ]
  }